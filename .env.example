# ================================
# HackPrinceton Environment Variables
# ================================
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control!

# ================================
# xAI Grok API Configuration (PRIMARY)
# ================================
# Your xAI API key for Grok Vision (receipt processing)
# Get your key at: https://x.ai/api or https://console.x.ai
XAI_API_KEY=your-xai-api-key-here

# Optional: Override xAI API URL (default: https://api.x.ai/v1)
# XAI_API_URL=https://api.x.ai/v1

# Optional: Specify Grok model (default: grok-vision-beta)
# XAI_MODEL=grok-vision-beta

# ================================
# OpenAI API (Alternative - if you want to use OpenAI instead)
# ================================
# OPENAI_API_KEY=sk-your-openai-api-key-here
# OPENAI_MODEL=gpt-4o

# ================================
# Capital One Nessie API
# ================================
# Capital One's Nessie API for banking data
# Get your key at: http://api.nessieisreal.com/
# NESSIE_API_KEY=your-nessie-api-key-here

# ================================
# Database Configuration (if needed)
# ================================
# DATABASE_URL=postgresql://user:password@localhost:5432/dbname
# MONGODB_URI=mongodb://localhost:27017/hackprinceton
# REDIS_URL=redis://localhost:6379

# ================================
# Authentication (if you add auth)
# ================================
# NEXTAUTH_SECRET=your-nextauth-secret-here
# NEXTAUTH_URL=http://localhost:3000

# OAuth Providers (examples)
# GOOGLE_CLIENT_ID=your-google-client-id
# GOOGLE_CLIENT_SECRET=your-google-client-secret
# GITHUB_CLIENT_ID=your-github-client-id
# GITHUB_CLIENT_SECRET=your-github-client-secret

# ================================
# Payment Processing (if needed)
# ================================
# STRIPE_PUBLIC_KEY=pk_test_your-stripe-public-key
# STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
# STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# ================================
# Email Service (if needed)
# ================================
# SENDGRID_API_KEY=your-sendgrid-api-key
# RESEND_API_KEY=your-resend-api-key

# ================================
# Cloud Storage (if needed)
# ================================
# AWS_ACCESS_KEY_ID=your-aws-access-key
# AWS_SECRET_ACCESS_KEY=your-aws-secret-key
# AWS_REGION=us-east-1
# AWS_S3_BUCKET=your-bucket-name

# Cloudinary
# CLOUDINARY_CLOUD_NAME=your-cloud-name
# CLOUDINARY_API_KEY=your-api-key
# CLOUDINARY_API_SECRET=your-api-secret

# ================================
# Analytics & Monitoring
# ================================
# NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX
# SENTRY_DSN=your-sentry-dsn

# ================================
# Rate Limiting & Security
# ================================
# RATE_LIMIT_DELAY=1000
# MAX_FILE_SIZE=10485760

# ================================
# Development Settings
# ================================
NODE_ENV=development

# ================================
# Receipt Processing Settings
# ================================
# Maximum number of receipts to process in one batch
# MAX_RECEIPTS_PER_BATCH=50

# Delay between processing receipts (milliseconds)
# RECEIPT_PROCESSING_DELAY=1000

# Enable auto-processing when new images arrive
# AUTO_PROCESS_RECEIPTS=false

# ================================
# Custom API Keys (add yours here)
# ================================
# Add any other API keys your project needs
